{
  "compilerOptions": {
      "module": "commonjs",
      "esModuleInterop": true,
      "target": "ES2020",
      "noImplicitAny": true,
      "moduleResolution": "node",
      "sourceMap": true,
      "outDir": ".",
      "baseUrl": ".",
      "resolveJsonModule":true
  },
  "include": [
      "src/*.ts","src/*.js","src/public/*.js"
  ],
  "exclude": ["node_modules"],
  "types":["node"]
  
}